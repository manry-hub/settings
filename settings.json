{
  "workbench.colorCustomizations": {
    "editor.selectionBackground": "#c6d0d33f",
    "editor.background": "#000000",
    "statusBar.background": "#292929",
    "tab.inactiveBackground": "#000000",
    "tab.border": "#494949",
    "sideBarTitle.background": "#000000",
    "activityBar.background": "#000000",
    "sideBar.background": "#000000",
    "panel.background": "#000000",
    "terminal.background": "#000000",
    "statusBarItem.remoteBackground": "#585857"
  },
  "workbench.iconTheme": "material-icon-theme",
  "chat.editor.fontSize": 17,
  "editor.fontSize": 13,
  "editor.lineHeight": 23,
  "editor.fontFamily": "Fira Code",
  "editor.linkedEditing": true,
  "editor.minimap.sectionHeaderFontSize": 19,
  "editor.autoIndent": "full",
  "editor.cursorBlinking": "expand",
  "editor.cursorStyle": "line",
  "editor.codeLensFontFamily": "Fira Code, monospace",
  "editor.fontLigatures": false,
  "editor.formatOnPaste": true,
  "editor.formatOnType": false,
  "editor.formatOnSave": true,
  "editor.letterSpacing": 0.4,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.wordWrap": "on",
  "search.showLineNumbers": true,
  "zenMode.centerLayout": false,
  "json.schemas": [],
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 15000,
  "bitoAI.appearance.fontSize (Match with IDE Font)": false,
  "workbench.settings.applyToAllProfiles": [
    "workbench.startupEditor",
    "codesnap.transparentBackground"
  ],
  "workbench.startupEditor": "none",
  "editor.guides.bracketPairs": true,
  "react-native-tools.showUserTips": false,
  "redhat.telemetry.enabled": true,
  "workbench.colorTheme": "One Dark Pro Darker",
  "codesnap.transparentBackground": true,
  "window.commandCenter": false,
  "glassit.alpha": 130,
  "editor.tabSize": 5,
  "workbench.layoutControl.enabled": false,
  "editor.minimap.size": "fit",
  "oneDarkPro.bold": true,
  "explorer.compactFolders": false,
  "workbench.editor.wrapTabs": true,
  "workbench.editor.enablePreview": false,
  "workbench.editor.pinnedTabsOnSeparateRow": true,
  "extensions.autoUpdate": false,
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "tailwindCSS.includeLanguages": {
    "plaintext": "html"
  },
  "path-intellisense.showHiddenFiles": true,
  "npm-intellisense.packageSubfoldersIntellisense": true,
  "editor.minimap.autohide": true,
  "tabnine.experimentalAutoImports": true,
  "javascript.updateImportsOnFileMove.enabled": "never"
}
