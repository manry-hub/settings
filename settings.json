{
    "workbench.colorCustomizations": {
        "editor.selectionBackground": "#c6d0d33f",
        "editor.background": "#000000",
        "statusBar.background": "#292929",
        "tab.inactiveBackground": "#000000",
        "tab.border": "#494949",
        "sideBarTitle.background": "#000000",
        "activityBar.background": "#000000",
        "sideBar.background": "#000000",
        "panel.background": "#000000",
        "terminal.background": "#000000",
        "statusBarItem.remoteBackground": "#585857"
    },
    "workbench.iconTheme": "material-icon-theme",
    "chat.editor.fontSize": 17,
    "editor.fontSize": 13,
    "editor.lineHeight": 23,
    "editor.fontFamily": "Fira Code",
    "editor.linkedEditing": true,
    "editor.minimap.sectionHeaderFontSize": 19,
    "editor.autoIndent": "full",
    "editor.cursorBlinking": "expand",
    "editor.cursorStyle": "line",
    "editor.codeLensFontFamily": "Fira Code, monospace",
    "editor.fontLigatures": false,
    "editor.formatOnType": false,
    "editor.letterSpacing": 0.4,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "on",
    "editor.wordWrap": "on",
    "search.showLineNumbers": true,
    "zenMode.centerLayout": false,
    "json.schemas": [],
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 15000,
    "workbench.settings.applyToAllProfiles": ["workbench.startupEditor", "codesnap.transparentBackground"],
    "workbench.startupEditor": "none",
    "editor.guides.bracketPairs": true,
    "workbench.colorTheme": "One Dark Pro Darker",
    "codesnap.transparentBackground": true,
    "window.commandCenter": false,
    "editor.tabSize": 8,
    "workbench.layoutControl.enabled": false,
    "editor.minimap.size": "fit",
    "oneDarkPro.bold": true,

    "extensions.autoUpdate": false,
    "files.associations": {
        "*.css": "tailwindcss"
    },
    "editor.quickSuggestions": {
        "strings": "on"
    },
    "tailwindCSS.includeLanguages": {
        "plaintext": "html"
    },
    "path-intellisense.showHiddenFiles": true,
    "npm-intellisense.packageSubfoldersIntellisense": true,
    "editor.minimap.autohide": true,
    "workbench.editor.enablePreview": true,

    "javascript.updateImportsOnFileMove.enabled": "never",
    "workbench.editor.pinnedTabsOnSeparateRow": true,
    "workbench.editor.wrapTabs": true,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.enablePreviewFromCodeNavigation": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "terminal.integrated.env.linux": {},
    "prettier.tabWidth": 4,
    "material-icon-theme.folders.theme": "classic",
    "material-icon-theme.opacity": 0.8,

    "files.exclude": {
        "**/node_modules": true,
        "**/.next": true,
        "**/.turbo": true,
        "**/.vercel": true,
        "**/.expo": true,
        "**/packages/**/node_modules": true,
        "**/dist": true,
        "**/build": true,
        "**/yarn.lock": false,
        "**/prisma/migrations": true,
        "**/coverage": true,
        "**/logs": true,
        "**/tmp": true,
        "**/.cache": true,
        "**/.eslintcache": true
    },
    "editor.formatOnSave": true,
    "window.newWindowProfile": "Node + Ts",
    "prettier.printWidth": 150,
    "continue.enableTabAutocomplete": false,
    "continue.showInlineTip": false
}
